{
  "presets": [
    ["es2015", { "loose": true, "modules": false }],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-object-rest-spread",
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties"
      ]
    }
  }
}